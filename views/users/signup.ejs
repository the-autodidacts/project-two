<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Sign up with valid company email</h3>
  </div>
  <div class="panel-body">
    <form class="" action="/users" method="post">
      <label for="user[fistName]">First Name</label>
      <input type="text"
             name="user[firstName]"
             placeholder="First Name">
      <label for="user[lastName]">Last Name</label>
      <input type="text"
             name="user[lastName]"
             placeholder="Last Name"><br/>
      <label for="email">email</label><br/>
      <input type="text"
             name="user[email]"
             placeholder="company email"><br/>
      <label for="password">Password</label><br/>
      <input type="password" name="user[password]" ><br/>
      <label for="password">Re-enter Password</label><br/>
      <input type="password" name="user[password]" ><br/>
      <input type="submit" name="name" value="submit">
    </form>

  </div>
</div>
